<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="svnadmin" version="2.5">
	<display-name>svnadmin</display-name>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>/WEB-INF/applicationContext.xml</param-value>
	</context-param>
	<filter>
		<filter-name>encodingFilter</filter-name>
		<filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
		<init-param>
			<param-name>forceEncoding</param-name>
			<param-value>true</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>encodingFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<listener>
		<listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
	</listener>

	<session-config>
		<session-timeout>60</session-timeout>
	</session-config>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	<error-page>
		<error-code>500</error-code>
		<location>/error.jsp</location>
	</error-page>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>org.svnadmin.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>usr</display-name>
		<servlet-name>usr</servlet-name>
		<servlet-class>org.svnadmin.servlet.UsrServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>usr</servlet-name>
		<url-pattern>/usr</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>pj</display-name>
		<servlet-name>pj</servlet-name>
		<servlet-class>org.svnadmin.servlet.PjServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pj</servlet-name>
		<url-pattern>/pj</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>pjusr</display-name>
		<servlet-name>pjusr</servlet-name>
		<servlet-class>org.svnadmin.servlet.PjUsrServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pjusr</servlet-name>
		<url-pattern>/pjusr</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>pjgr</display-name>
		<servlet-name>pjgr</servlet-name>
		<servlet-class>org.svnadmin.servlet.PjGrServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pjgr</servlet-name>
		<url-pattern>/pjgr</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>pjgrusr</display-name>
		<servlet-name>pjgrusr</servlet-name>
		<servlet-class>org.svnadmin.servlet.PjGrUsrServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pjgrusr</servlet-name>
		<url-pattern>/pjgrusr</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>pjauth</display-name>
		<servlet-name>pjauth</servlet-name>
		<servlet-class>org.svnadmin.servlet.PjAuthServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>pjauth</servlet-name>
		<url-pattern>/pjauth</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>lang</display-name>
		<servlet-name>lang</servlet-name>
		<servlet-class>org.svnadmin.servlet.LangServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>lang</servlet-name>
		<url-pattern>/lang</url-pattern>
	</servlet-mapping>
</web-app>